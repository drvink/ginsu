module Version(package, version, fullName, bugReport, prefix) where

{- # NOTINLINE package #-}
{- # NOTINLINE version #-}
{- # NOTINLINE fullName #-}
{- # NOTINLINE bugReport #-}
{- # NOTINLINE prefix #-}

package :: String
package = "@PACKAGE@"

version :: String
version = "@VERSION@"

bugReport :: String
bugReport = "@PACKAGE_BUGREPORT@"

fullName :: String
fullName = "@PACKAGE@-@VERSION@"

prefix :: String
prefix = "@PREFIX@"
